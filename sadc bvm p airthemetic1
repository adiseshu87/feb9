#include <stdio.h>

int main()
{
    int a=5,*p;
    p=&a;
    printf("value of p is %u\n",p);
    printf("value of p+1 is %u\n",p+1);
    printf("value of p-1 is %u\n",p-1);
    printf("value of p+2 is %u\n",p+2);
    printf("value of p-2 is %u\n",p-2);
    printf("value of p+4 is %u\n",p+4);
    printf("value of p-4 is %u\n",p+2);
    
}
